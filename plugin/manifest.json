{
  "id": "b111e821",
  "name": "WatermarkRemover.io",
  "version": "1.0.5",
  "main": "index.html",
  "manifestVersion": 5,
  "host": [
    {
      "app": "PS",
      "minVersion": "23.3.0"
    }
  ],
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "launchProcess": {
      "schemes": ["http", "https"],
      "extensions": [".svg", ".png"]
    },
    "network": {
      "domains": [
        "https://console.pixelbin.io",
        "https://api.pixelbin.io",
        "https://cdn.pixelbin.io",
        "https://www.pixelbin.io",
        "https://www.watermarkremover.io"
      ]
    }
  },
  "entrypoints": [
    {
      "type": "command",
      "id": "goToDashboard",
      "label": { "default": "Go to dashboard" }
    },
    {
      "type": "command",
      "id": "buyCredits",
      "label": { "default": "Buy credits" }
    },
    {
      "type": "command",
      "id": "howItWorks",
      "label": { "default": "How it works" }
    },
    {
      "type": "panel",
      "id": "app",
      "label": { "default": "WatermarkRemover.io" },
      "minimumSize": { "width": 260, "height": 560 },
      "maximumSize": { "width": 260, "height": 560 },
      "preferredDockedSize": { "width": 260, "height": 560 },
      "preferredFloatingSize": { "width": 260, "height": 560 },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/watermarkremover.png",
          "scale": [1, 2],
          "theme": ["all"],
          "species": ["generic"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/watermarkremover.png",
      "scale": [1, 2],
      "theme": ["all"],
      "species": ["generic"]
    }
  ]
}
